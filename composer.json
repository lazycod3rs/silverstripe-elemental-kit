{
    "name": "lazycoders/silverstripe-elemental-kit",
    "description": "Elemental Kit",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Elemental",
        "Content Blocks"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "dnadesign/silverstripe-elemental": "^4.5.0",
        "sheadawson/silverstripe-linkable": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LazyCoders\\Elemental\\": "src/",
            "LazyCoders\\Elemental\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
